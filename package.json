{
	"name": "wp-block-cards",
	"version": "1.3.2",
	"description": "Grid or carousel with card blocks which have an image, title, description and link.",
	"author": "SMFB Dinamo",
	"license": "GPL-2.0-or-later",
	"main": "build/index.js",
	"scripts": {
		"build:card": "wp-scripts build blocks/card/src/index.js --output-path=blocks/card/build/",
		"build:container": "wp-scripts build blocks/container/src/index.js --output-path=blocks/container/build/",
		"build": "npm run build:card && npm run build:container",
		"format": "wp-scripts format",
		"lint:css": "wp-scripts lint-style",
		"lint:css:fix": "wp-scripts lint-style --fix",
		"lint:js": "wp-scripts lint-js",
		"lint:js:fix": "wp-scripts lint-js --fix",
		"start:card": "wp-scripts start blocks/card/src/index.js --output-path=blocks/card/build/",
		"start:container": "wp-scripts start blocks/container/src/index.js --output-path=blocks/container/build/",
		"packages-update": "wp-scripts packages-update"
	},
	"dependencies": {
		"@wordpress/block-editor": "^12.3.0",
		"@wordpress/blocks": "^12.12.0",
		"@wordpress/components": "^25.1.0",
		"@wordpress/dom-ready": "^3.35.0",
		"@wordpress/element": "^5.12.0",
		"@wordpress/hooks": "^3.35.0",
		"@wordpress/i18n": "^4.35.0",
		"@wordpress/icons": "^9.26.0",
		"@wordpress/primitives": "^3.33.0",
		"lodash": "^4.17.21"
	},
	"devDependencies": {
		"@wordpress/scripts": "^26.6.0",
		"prettier": "^2.5.1"
	}
}
